  FormBox[
   RowBox[{
    SubscriptBox["\[Lambda]", "1"], "=", "0.75"}], TraditionalForm]],
  ExpressionUUID->"818b9005-7ca4-4db5-8bd6-b8e4a5e35ec6"],
 ", ",
