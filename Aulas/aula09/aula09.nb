 Cell[BoxData[
  FormBox["1024", TraditionalForm]],ExpressionUUID->
  "d3d8de34-057e-40c6-a969-cfac3a6b7471"],
 " valores de ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "a6d2bde0-a91d-4d92-8187-11112994dfe6"],
 " obtidos pela itera\[CCedilla]\[ATilde]o do mapa ",
 Cell[BoxData[
  FormBox["2048", TraditionalForm]],ExpressionUUID->
  "4ddfe163-5bfd-45cf-afce-2fdd19bfba9f"],
 " vezes."
}], "Text",
