 RowBox[{
  RowBox[{
   RowBox[{"subs", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Gamma]", "\[Rule]", " ", "0.20"}], ",", 
      RowBox[{"\[Omega]0", "\[Rule]", " ", "2"}], ",", 
      RowBox[{"A", "\[Rule]", " ", "1"}]}], "}"}]}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sol", "=", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"x", "''"}], "[", "t", "]"}], "+", 
          RowBox[{"2", "*", "\[Gamma]", "*", 
           RowBox[{
            RowBox[{"x", "'"}], "[", "t", "]"}], "*", 
           RowBox[{"Abs", "[", 
            RowBox[{
             RowBox[{"x", "'"}], "[", "t", "]"}], "]"}]}], "+", 
          RowBox[{
           RowBox[{"\[Omega]0", "^", "2"}], "*", 
           RowBox[{"x", "[", "t", "]"}]}]}], "\[Equal]", "0"}], ",", 
        RowBox[{
         RowBox[{"x", "[", "0", "]"}], "\[Equal]", "A"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"x", "'"}], "[", "0", "]"}], "\[Equal]", "0"}]}], "}"}], "/.",
       "subs"}], ",", "x", ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "15"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pos", "[", "t_", "]"}], ":=", 
  RowBox[{
   RowBox[{"x", "[", "t", "]"}], "/.", 
   RowBox[{"sol", "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
