Cell[BoxData[
 RowBox[{
  RowBox[{"grafdif", "[", 
   RowBox[{"\[Lambda]_", ",", "x_", ",", "\[Epsilon]_", ",", "n_"}], "]"}], ":=",
   " ", "\[IndentingNewLine]", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"xa", "=", "x"}], ";", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
