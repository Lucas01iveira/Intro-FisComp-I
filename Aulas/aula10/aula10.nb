Cell[BoxData[
 RowBox[{
  RowBox[{"lyapunov", "[", 
   RowBox[{"\[Lambda]_", ",", "x0_", ",", "n_", ",", "m_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"xlista", "=", 
     RowBox[{"Drop", "[", 
      RowBox[{
       RowBox[{"NestList", "[", 
        RowBox[{
         RowBox[{"f", "[", "\[Lambda]", "]"}], ",", "x0", ",", "n"}], "]"}], 
       ",", 
       RowBox[{"m", "+", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Total", "[", 
      RowBox[{"Log", "[", 
       RowBox[{"Abs", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"f", "[", "\[Lambda]", "]"}], "'"}], "[", "xlista", "]"}], 
        "]"}], "]"}], "]"}], "/", 
     RowBox[{"Length", "[", "xlista", "]"}]}]}], "\[IndentingNewLine]", ")"}],
   "  "}]], "Input",
