Cell[BoxData[{
 RowBox[{
  RowBox[{"eq", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"x", "''"}], "[", "t", "]"}], "+", 
     RowBox[{"\[Gamma]", "*", 
      RowBox[{
       RowBox[{"x", "'"}], "[", "t", "]"}]}], "+", 
     RowBox[{
      RowBox[{"\[Omega]0", "^", "2"}], "*", 
      RowBox[{"x", "[", "t", "]"}]}]}], "\[Equal]", "0"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Assuming", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Omega]0", ">", "0"}], ",", 
      RowBox[{"\[Gamma]", ">", "0"}], ",", 
      RowBox[{"\[Gamma]", "<", 
       RowBox[{"\[Omega]0", "/", "2"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"DSolve", "[", 
      RowBox[{"eq", ",", 
       RowBox[{"x", "[", "t", "]"}], ",", "t"}], "]"}], "//", 
     "FullSimplify"}]}], "]"}], " "}]}], "Input",
