  3.84400429838899*^9}},ExpressionUUID->"5ef29a2a-0330-4988-8c52-\
173723bc616f"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"eq2", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"x", "''"}], "[", "t", "]"}], "+", 
     RowBox[{"\[Rho]", "*", 
      RowBox[{
       RowBox[{"x", "'"}], "[", "t", "]"}], "*", 
      RowBox[{"Abs", "[", 
       RowBox[{
        RowBox[{"x", "'"}], "[", "t", "]"}], "]"}]}], "+", 
     RowBox[{
      RowBox[{"\[Omega]0", "^", "2"}], "*", 
      RowBox[{"x", "[", "t", "]"}]}]}], "\[Equal]", "0"}]}], ";", " ", 
  RowBox[{"sol2", "=", 
   RowBox[{"ParametricNDSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"eq2", ",", 
       RowBox[{
        RowBox[{"x", "[", "0", "]"}], "\[Equal]", "x0"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"x", "'"}], "[", "0", "]"}], "\[Equal]", "v0"}]}], "}"}], 
     ",", "x", ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "11"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Rho]", ",", "x0", ",", "v0", ",", "\[Omega]0"}], "}"}]}], 
    "]"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Podemos", " ", "invocar", " ", "a", " ", "solu\[CCedilla]\[ATilde]o", 
     " ", "como", " ", 
     RowBox[{
      RowBox[{"x", "[", 
       RowBox[{"\[Rho]", ",", "x0", ",", "v0", ",", "\[Omega]0"}], "]"}], "[",
       "t", "]"}]}], " ", "*)"}]}]}]], "Input",
 CellChangeTimes->{{3.8439317592897377`*^9, 3.843931818880179*^9}, {
   3.844002641986162*^9, 3.844002716520341*^9}, {3.844002746692165*^9, 
